FILE(GLOB INSTALLED_LIBNAME $ENV{DESTDIR}@CMAKE_INSTALL_PREFIX@/@INSTALL_LIB_DIR@/*@BEFORE_RENAME@.*)
MESSAGE("INSTALLED_LIBNAME  $ENV{DESTDIR}@CMAKE_INSTALL_PREFIX@/@INSTALL_LIB_DIR@/*@BEFORE_RENAME@.*")
MESSAGE("INSTALLED_LIBNAME  ${INSTALLED_LIBNAME}")
STRING(REPLACE "@BEFORE_RENAME@" "@AFTER_RENAME@" INSTALLED_NAME_AFTER_RENAME ${INSTALLED_LIBNAME})
EXEC_PROGRAM(@CMAKE_COMMAND@ ARGS -E copy "${INSTALLED_LIBNAME}" "${INSTALLED_NAME_AFTER_RENAME}")
EXEC_PROGRAM(@CMAKE_COMMAND@ ARGS -E remove "${INSTALLED_LIBNAME}")

